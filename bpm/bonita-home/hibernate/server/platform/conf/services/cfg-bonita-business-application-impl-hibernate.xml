<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="hbm-bos-page-model" class="org.bonitasoft.engine.persistence.HibernateResourcesProvider">
		<property name="resources">
			<set>
				<value>org/bonitasoft/engine/business/application/impl/hibernate/business.application.hbm.xml</value>
				<value>org/bonitasoft/engine/business/application/impl/hibernate/business.application.queries.hbm.xml</value>
			</set>
		</property>
		<property name="classAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.business.application.SBusinessApplication" value="businessApp" />
				<entry key="org.bonitasoft.engine.business.application.SBusinessApplicationPage" value="businessAppPage" />
			</map>
		</property>
	</bean>

</beans>