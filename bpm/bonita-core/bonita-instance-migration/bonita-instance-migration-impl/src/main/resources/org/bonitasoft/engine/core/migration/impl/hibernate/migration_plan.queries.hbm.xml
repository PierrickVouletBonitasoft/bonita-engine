<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false">

    <query name="getNumberOfMigrationPlanDescriptors">
        SELECT COUNT(mp)
        FROM org.bonitasoft.engine.core.migration.model.impl.SMigrationPlanDescriptorImpl AS mp
    </query>
    <query name="getMigrationPlanDescriptors">
        SELECT mp
        FROM org.bonitasoft.engine.core.migration.model.impl.SMigrationPlanDescriptorImpl AS mp
    </query>
</hibernate-mapping>
